    <!--CANOPY-->
    <animation>
        <type>pick</type>
        <object-name>window.ext</object-name>
        <object-name>window.int</object-name>
        <object-name>canopy</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>doors.canopyF.toggle();</script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>window.ext</object-name>
        <object-name>canopy</object-name>
        <property>sim/model/door-positions/canopyF/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>-1</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-80</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-3.27191</x1-m>
            <y1-m>0.28064</y1-m>
            <z1-m>-0.24765</z1-m>
            <x2-m>-1.84713</x2-m>
            <y2-m>0.3468</y2-m>
            <z2-m>-0.14673</z2-m>
        </axis>
    </animation>
    <animation>
        <type>pick</type>
        <object-name>canopy.backframe</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>doors.canopyB.toggle();</script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>canopy.backframe</object-name>
        <object-name>canopy.back2</object-name>
        <property>sim/model/door-positions/canopyB/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>50</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
        <center>
            <x-m>-1.26909</x-m>
            <z-m>1.11</z-m>
        </center>
    </animation>
